/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "linksys,ea8100", "mediatek,mt7621-soc";
	model = "Linksys EA8100";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

};

&nand {
	status = "okay";

	partition@0 {
		label = "uboot";
		reg = <0x00000 0x80000>;
	};

	partition@80000 {
		label = "uboot_env";
		reg = <0x80000 0x40000>;
	};

	partition@c0000 {
		label = "factory";
		reg = <0xc0000 0x40000>;
	};

	partition@100000 {
		label = "s_env";
		reg = <0x100000 0x40000>;
	};

	partition@140000 {
		label = "devinfo";
		reg = <0x140000 0x40000>;
	};

	partition@180000 {
		label = "firmware";
		reg = <0x180000 0x2800000>;
	};

	/*
	partition@180000 {
		label = "kernel";
		reg = <0x180000 0x2800000>;
	};

	partition@580000 {
		label = "rootfs";
		reg = <0x580000 0x2400000>;
	};
	 */
};

/*
